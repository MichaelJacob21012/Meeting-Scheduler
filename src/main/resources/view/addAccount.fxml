<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddAccountController">
    <children>
        <AnchorPane prefHeight="500.0" prefWidth="650.0">
            <children>
                <MenuBar fx:id="adminMenuBar" prefHeight="25.0" prefWidth="650.0">
                    <menus>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyCalendar" text="My Calendar" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToAddMeeting" text="Add Meeting" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToInvites" text="My Invites" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyAccount" text="My Account" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Accounts">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#goToAddAccount" text="Add Account" />
                                <MenuItem mnemonicParsing="false" onAction="#goToAccounts" text="Accounts List" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Rooms">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#goToAddRoom" text="Add Room" />
                                <MenuItem mnemonicParsing="false" onAction="#goToRooms" text="Rooms List" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Locations">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#goToAddLocation" text="Add Location" />
                                <MenuItem mnemonicParsing="false" onAction="#goToLocations" text="Locations List" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" onAction="#goToBatch">
                            <graphic>
                                <Label onMouseClicked="#goToBatch" text="Batch" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#logout" text="Logout" />
                            </graphic>
                        </Menu>
                    </menus>
                </MenuBar>
                <MenuBar fx:id="normalMenuBar" prefHeight="25.0" prefWidth="650.0">
                    <menus>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyCalendar" text="My Calendar" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToAddMeeting" text="Add Meeting" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToInvites" text="My Invites" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyAccount" text="My Account" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToRooms" text="Rooms" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#logout" text="Logout" />
                            </graphic>
                        </Menu>
                    </menus>
                </MenuBar>
                <Button fx:id="submitButton" layoutX="62.0" layoutY="391.0" mnemonicParsing="false" onMouseClicked="#submit" prefHeight="16.0" prefWidth="131.0" text="Submit" />
            <TextField fx:id="firstNameField" layoutX="34.0" layoutY="52.0" prefHeight="40.0" prefWidth="238.0" promptText="First Name" />
            <TextField fx:id="surnameField" layoutX="34.0" layoutY="113.0" prefHeight="40.0" prefWidth="238.0" promptText="Surname" />
            <PasswordField fx:id="passwordField" layoutX="34.0" layoutY="230.0" prefHeight="40.0" prefWidth="238.0" promptText="Password" />
            <TextField fx:id="emailField" layoutX="34.0" layoutY="171.0" prefHeight="40.0" prefWidth="238.0" promptText="Email" />
            <CheckBox fx:id="adminBox" layoutX="34.0" layoutY="349.0" mnemonicParsing="false" text="Admin" />
            <Label fx:id="firstNameLabel" layoutX="284.0" layoutY="67.0" prefHeight="25.0" prefWidth="266.0" textFill="#eb0404" />
            <Label fx:id="surnameLabel" layoutX="284.0" layoutY="133.0" prefHeight="25.0" prefWidth="266.0" textFill="#eb0404" />
            <Label fx:id="emailLabel" layoutX="284.0" layoutY="198.0" prefHeight="25.0" prefWidth="266.0" textFill="#eb0404" />
            <Label fx:id="passwordLabel" layoutX="284.0" layoutY="257.0" prefHeight="25.0" prefWidth="266.0" textFill="#eb0404" />
            <PasswordField fx:id="confirmField" layoutX="34.0" layoutY="289.0" prefHeight="40.0" prefWidth="238.0" promptText="Confirm Password" />
            </children>
        </AnchorPane>
    </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
