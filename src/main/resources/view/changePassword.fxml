                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Rooms">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#goToAddRoom" text="Add Room" />
                                <MenuItem mnemonicParsing="false" onAction="#goToRooms" text="Rooms List" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Locations">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#goToAddLocation" text="Add Location" />
                                <MenuItem mnemonicParsing="false" onAction="#goToLocations" text="Locations List" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" onAction="#goToBatch">
                            <graphic>
                                <Label onMouseClicked="#goToBatch" text="Batch" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#logout" text="Logout" />
                            </graphic>
                        </Menu>
                    </menus>
                </MenuBar>
                <MenuBar fx:id="normalMenuBar" prefHeight="25.0" prefWidth="637.0">
                    <menus>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyCalendar" text="My Calendar" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToAddMeeting" text="Add Meeting" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToInvites" text="My Invites" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToMyAccount" text="My Account" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#goToRooms" text="Rooms" />
                            </graphic>
                        </Menu>
                        <Menu mnemonicParsing="false">
                            <graphic>
                                <Label onMouseClicked="#logout" text="Logout" />
                            </graphic>
                        </Menu>
                    </menus>
                </MenuBar>
                <Button fx:id="confirmButton" layoutX="319.0" layoutY="250.0" mnemonicParsing="false" onMouseClicked="#confirm" prefHeight="25.0" prefWidth="75.0" text="Confirm" />
                <PasswordField fx:id="passwordField" layoutX="191.0" layoutY="135.0" prefHeight="20.0" prefWidth="200.0" promptText="Password" />
                <PasswordField fx:id="confirmField" layoutX="191.0" layoutY="190.0" prefHeight="20.0" prefWidth="200.0" promptText="Confirm password" />
                <Button fx:id="cancelButton" layoutX="191.0" layoutY="250.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="25.0" prefWidth="75.0" text="Cancel" />
            <Label fx:id="errorLabel" layoutX="429.0" layoutY="139.0" prefHeight="17.0" prefWidth="159.0" textFill="#eb0404" />
            </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
