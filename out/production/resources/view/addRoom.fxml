            <TextField fx:id="locationField" layoutX="34.0" layoutY="106.0" prefHeight="25.0" prefWidth="224.0" promptText="Location" />
            <TextField fx:id="descriptionField" layoutX="34.0" layoutY="203.0" prefHeight="47.0" prefWidth="578.0" promptText="Description" />
